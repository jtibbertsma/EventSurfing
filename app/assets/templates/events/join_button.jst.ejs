<% if (event.join) { %>
  <% if (event.get("organizer_id") !== event.join.get("attender_id")) { %>
    <button class="btn btn-default cool-button unjoin">Unjoin</button>
  <% } %>
<% } else if (event.get("spots_remaining") !== 0) { %>
  <button class="btn btn-default cool-button join">Join</button>
<% } %>