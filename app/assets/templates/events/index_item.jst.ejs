<div class="index-item-text">
  <h4><%= event.escape("title") %></h4>
  <p>Location will go here</p>
  <p><%= event.get("start_time") %></p>
  <p><%= event.get("num_attenders") %> attending</p>
</div>
<div class="index-item-join">
  <% if (PadCrashing.CURRENT_USER.id !== event.organizer_id) { %>
    <button class="btn btn-default join">Join</button>
  <% } else { %>
    <button class="btn btn-default unjoin">Unjoin</button>
  <% } %>
</div>